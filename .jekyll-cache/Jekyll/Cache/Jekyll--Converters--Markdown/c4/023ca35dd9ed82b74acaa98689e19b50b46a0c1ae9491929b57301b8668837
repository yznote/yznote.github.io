I"<<h5 id="nsdictionary扩展">NSDictionary扩展</h5>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>@implementation NSDictionary (RKLogJson)

#if DEBUG
- (NSString *)descriptionWithLocale:(nullable id)locale indent:(NSUInteger)level{
    if ([NSJSONSerialization isValidJSONObject:self]) {
        return [[NSString alloc] initWithData:[NSJSONSerialization dataWithJSONObject:self options:NSJSONWritingPrettyPrinted error:nil] encoding:NSUTF8StringEncoding];
    }
    return @"rk=invalid json";
}
#endif
@end
</code></pre></div></div>
