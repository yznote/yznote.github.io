    struct Birthday{
        int year;
        int month;
        int day;
    };
    struct Student {
        int age;
        char *name;
        struct Birthday bir1;
    };
    struct Student stu1={21,"Jack",{1991,10,10}};
    printf("age=%d,name=%s,birthday:%d-%d-%d",stu1.age,stu1.name,stu1.bir1.year,stu1.bir1.month,stu1.bir1.day);
}
