    [userDefaults setObject:save forKey:@"ybcos_savetime"];
    [userDefaults synchronize];
}
+(NSDate *)getResTime {
    NSUserDefaults *userDefaults = [NSUserDefaults standardUserDefaults];
    NSDate *getDate = [userDefaults objectForKey:@"ybcos_savetime"];
    return getDate;
}
