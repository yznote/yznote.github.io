<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>Person.m
#import "Person.h"
@implementation Person
-(void)setAge:(int)newAge andName:(NSString *)newName
{
    age=newAge;
    name=newName;
}
-(int)getAge
{
    return age;
}
-(NSString*)getName
{
    return name;
}
@end
</code></pre></div></div>
