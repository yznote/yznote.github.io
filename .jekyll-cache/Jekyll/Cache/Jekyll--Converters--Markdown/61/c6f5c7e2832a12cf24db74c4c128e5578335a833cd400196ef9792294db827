#import
@interface Person : NSObject
{
    int age;
    NSString *name;
}
//è®¾ç½®å™¨
//-(void)setAge:(int)newAge;
//è®¿é—®å™¨
//-(int)getAge;
-(void)setAge:(int)newAge andName:(NSString *)newName;
