#import &lt;UIKit/UIKit.h&gt;

@interface RKPaintedGiftView : UIView

@property(nonatomic,strong)NSString *giftNameStr;           //ç¤¼ç‰©åç§°
@property(nonatomic,strong)NSString *giftPathStr;           //ç¤¼ç‰©åœ°å€

@property(nonatomic,assign)int minNum;                      //ç¤¼ç‰©æœ€å°‘ä¸ªæ•°ã€æš‚æ—¶æœªåšåˆ¤æ–­ã€‘
@property(nonatomic,assign)int maxNum;                      //ç¤¼ç‰©æœ€å¤§ä¸ªæ•°ã€æš‚æ—¶æœªåšåˆ¤æ–­ã€‘

@property(nonatomic,assign)BOOL showPainted;                //å±•ç¤ºç»˜åˆ¶åŒºåŸŸ

