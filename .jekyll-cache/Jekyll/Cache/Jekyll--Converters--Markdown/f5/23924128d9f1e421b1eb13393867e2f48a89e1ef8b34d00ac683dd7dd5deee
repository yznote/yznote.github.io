#import &lt;UIKit/UIKit.h&gt;

@interface RKShowPaintedView : UIView

@property(nonatomic,strong)NSString *giftPathStr;               //æ‰‹ç»˜ç¤¼ç‰©åœ°å€
@property(nonatomic,strong)NSArray *paintedPointArray;          //æ‰‹ç»˜ç¤¼ç‰©å›¾å½¢è·¯å¾„

@property(nonatomic,assign)CGFloat paintedWidth;                //å‘é€æ–¹æ‰‹ç»˜ç¤¼ç‰©åŒºåŸŸå®½åº¦
